<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>User:</strong>
  <%= @post.user_id %>
</p>


<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>

<table>
      <tr id="post_<%= @post.id %>">
        <%= render 'post', post: @post %>
      </tr>
</table>